#!/bin/bash
pod2man -c "Raspberry Pi" -r "version 20130909" pipic.pod pipic.1
pod2man -c "Raspberry Pi" -r "version 20130811" pipicfile.pod pipicfile.1
pod2man -c "Raspberry Pi" -r "version 20140709" --section=8 pipicpowerd.pod pipicpowerd.1
pod2man -c "Raspberry Pi" -r "version 20140224" pipicsw.pod pipicsw.1
pod2man -c "Raspberry Pi" -r "version 20140219" pipicswd.pod pipicswd.1
pod2man -c "Raspberry Pi" -r "version 20130819" pipictest.pod pipictest.1
pod2man -c "Raspberry Pi" -r "version 20140914" pipichbd.pod pipichbd.1

